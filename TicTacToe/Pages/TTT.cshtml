@page
@model TicTacToe.Pages.TTTModel
@{
    ViewData["Title"] = "TTT";
}

<h2>TTT</h2>

@for (int i = 0; i < Model.Board.GetLength(0); i++)
{
<div class="row">
    @for (int k = 0; k < Model.Board.GetLength(1); k++)
    {
        @if (Model.Board[i, k] == 0)
        {
            <a href="" class="square-2"></a>
        }
        else
        {
            <div class="square2">@Model.Board[i, k]</div>

        }
    }
</div>
}